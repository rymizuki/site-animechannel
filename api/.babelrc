{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "8.9"
      }
    }],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": ["babel-plugin-espower"]
    }
  }
}
